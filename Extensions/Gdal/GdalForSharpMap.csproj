  <PropertyGroup>
    <PreBuildEvent>echo off
setlocal
echo cd "$(DevEnvDir)..\..\VC" &gt; buildgdal.bat
echo set path=$(SolutionDir)BuildTools;%25PATH%25 &gt;&gt; buildgdal.bat
if $(PlatformName)=="x64" (
echo call vcvarsall.bat x86_amd64 &gt;&gt; buildgdal.bat 
) else (
echo call vcvarsall.bat x86 &gt;&gt; buildgdal.bat 
)
echo if "%251"=="GDAL" goto GDAL &gt;&gt; buildgdal.bat
echo cd $(ProjectDir)Proj4.6.1 &gt;&gt; buildgdal.bat
rem NMAKE.OPT für Proj4 Kompilierung
echo INSTDIR=$(TargetDir).. &gt; $(ProjectDir)Proj4.6.1\nmake.opt
echo PROJ_LIB_DIR=$(TargetDir)..\share &gt;&gt; $(ProjectDir)Proj4.6.1\nmake.opt
echo OPTFLAGS=/nologo /Ox /Op /MD &gt;&gt; $(ProjectDir)Proj4.6.1\nmake.opt
echo EXE_PROJ =proj_i.lib &gt;&gt; $(ProjectDir)Proj4.6.1\nmake.opt
echo VERSION= &gt;&gt; $(ProjectDir)Proj4.6.1\nmake.opt
echo nmake /f makefile.vc clean &gt;&gt; buildgdal.bat
echo nmake /f makefile.vc /x projbuild.err install-all &gt;&gt; buildgdal.bat
rem echo pause &gt;&gt; buildgdal.bat
echo :GDAL &gt;&gt; buildgdal.bat
echo cd $(ProjectDir)Gdal1.6.3 &gt;&gt; buildgdal.bat

REM EXT_NMAKE.OPT für GDAL Kompilierung
echo INSTDIR=$(TargetDir).. &gt; $(ProjectDir)Gdal1.6.3\ext_nmake.opt
if $(PlatformName)=="x64" echo WIN64=YES &gt;&gt; $(ProjectDir)Gdal1.6.3\ext_nmake.opt
echo MSVC_VER=1500 &gt;&gt; $(ProjectDir)Gdal1.6.3\ext_nmake.opt
echo GDAL_HOME = $(TargetDir).. &gt;&gt; $(ProjectDir)Gdal1.6.3\ext_nmake.opt

echo if "%252"=="SWIG" goto SWIG &gt;&gt; buildgdal.bat
echo nmake /f makefile.vc clean &gt;&gt; buildgdal.bat
echo nmake /f makefile.vc /x gdalbuild.err EXT_NMAKE_OPT=ext_nmake.opt &gt;&gt; buildgdal.bat
rem echo nmake /f makefile.vc /x install.err install EXT_NMAKE_OPT=ext_nmake.opt &gt;&gt; buildgdal.bat
echo nmake /f makefile.vc /x devinstall.err devinstall EXT_NMAKE_OPT=ext_nmake.opt &gt;&gt; buildgdal.bat
rem echo pause &gt;&gt; buildgdal.bat

echo :SWIG &gt;&gt; buildgdal.bat
echo cd swig &gt;&gt; buildgdal.bat
echo nmake /f makefile.vc /x gdalcsharpbindings.err csharp install EXT_NMAKE_OPT=ext_nmake.opt &gt;&gt; buildgdal.bat
echo cd csharp &gt;&gt; buildgdal.bat
echo nmake /f makefile.vc /x gdalcsharpinstall.err install EXT_NMAKE_OPT=ext_nmake.opt &gt;&gt; buildgdal.bat
echo endlocal
echo cd $(TargetDir) &gt;&gt; buildgdal.bat
echo on</PreBuildEvent>
